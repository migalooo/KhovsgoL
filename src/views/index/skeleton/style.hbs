<style id="snnu_skeleton_style">
/*---------- Default Style ----------*/
html{
  font-size: calc(100vw/7.5);
}
.snnu-skeleton{
  background: #fff;
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 900;
  text-align: center;
  font-size: 0;
}
.snnu-skeleton .greybar{
  background: #f9f9f9;
  display: inline-block;
}

/* Icon animation */
.snnu-skeleton .snnu-font-logo{
  font-size: .6rem;
  font-family: Arial;
  color: #f7f7f7;
  font-weight: 800;
  font-style: oblique;
  line-height: 1.2rem;
  margin-top: 1.5rem;
}
@keyframes logo{
  from { color: #fcfcfc; }
  to { color: #e9e9e9; }
}
@-webkit-keyframes logo{
  from { color: #fcfcfc; }
  to { color: #e9e9e9; }
}
.snnu-skeleton .snnu-font-logo>span:nth-child(1){
  animation: logo .5s ease-in-out  infinite alternate;
  -webkit-animation: logo .5s ease-in-out  infinite alternate;
}
.snnu-skeleton .snnu-font-logo>span:nth-child(2){
  animation: logo .5s ease-in-out .1s infinite alternate;
  -webkit-animation: logo .5s ease-in-out .1s infinite alternate;
}
.snnu-skeleton .snnu-font-logo>span:nth-child(3){
  animation: logo .5s ease-in-out .3s infinite alternate;
  -webkit-animation: logo .5s ease-in-out .3s infinite alternate;
}
.snnu-skeleton .snnu-font-logo>span:nth-child(4){
  animation: logo .5s ease-in-out .7s infinite alternate;
  -webkit-animation: logo .5s ease-in-out .7s infinite alternate;
}

/* Remove animation */
@keyframes fadeSkeleton{
  from {opacity: 1;}
  to {opacity: 0;}
}
@-webkit-keyframes fadeSkeleton{
  from {opacity: 1;}
  to {opacity: 0;}
}
.snnu-skeleton-fade {
  opacity: 0;
  animation: fadeSkeleton .3s ease-out;
  -webkit-animation: fadeSkeleton .3s ease-out;
}



/*---------- Custom Style ----------*/
.snnu-skeleton .words{
  margin-top: .4rem;
  width: 80%;
  height: .4rem;
}
.snnu-skeleton .row-warp{
  width: 100%;
  margin-top: .3rem;
}
.snnu-skeleton .square{
  width: 20%;
  padding-bottom: 20%;
  margin: 5%;
}
.snnu-skeleton .list-warp .words{
  width: 65%;
  
}
.snnu-skeleton .list-warp .button{
  width: 10%;
  height: .4rem;
  margin-left: 5%;
}

</style>
